{
  "name": "@fczbkk/url-match",
  "title": "UrlMatch",
  "version": "3.5.0",
  "description": "JavaScript object for matching URLs against patterns.",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/fczbkk/UrlMatch.git"
  },
  "author": {
    "name": "Riki Fridrich",
    "email": "riki@fczbkk.com",
    "url": "http://fczbkk.com/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fczbkk/UrlMatch/issues"
  },
  "homepage": "https://github.com/fczbkk/UrlMatch#readme",
  "scripts": {
    "cleanup": "node cleanup.mjs",
    "prebuild": "npm run test && npm run cleanup",
    "build": "node build.mjs",
    "docs": "node build.mjs",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "dev": "vitest",
    "lint": "eslint src test",
    "lint:fix": "eslint src test --fix",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.js\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"test/**/*.js\"",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "postversion": "git push && git push --tags && npm publish --access=public",
    "version": "npm run build && npm run changelog && git add -A",
    "doc": "documentation readme ./src/index.js -s 'Documentation'"
  },
  "main": "lib/index.js",
  "module": "esm/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./esm/index.js",
      "require": "./lib/index.js"
    }
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@vitest/coverage-v8": "^4.0.16",
    "conventional-changelog-cli": "^5.0.0",
    "documentation": "^14.0.3",
    "esbuild": "^0.27.2",
    "eslint": "^9.18.0",
    "prettier": "^3.4.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  }
}
