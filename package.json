  "version": "3.3.4",
  "description": "JavaScript object for matching URLs against patterns.",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/fczbkk/UrlMatch.git"
  },
  "author": {
    "name": "Riki Fridrich",
    "email": "riki@fczbkk.com",
